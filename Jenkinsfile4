pipeline{
          agent any
          stages{
		stage('Clone git repo'){
                    steps{
                          echo 'Clone git repo'
			  bat 'git clone https://github.com/venupall/VenuPipelineRepo.git '
                        }
                }
                stage('Compile'){
                    steps{
                          echo 'Compile'
                        }
                }
		stage('Test'){
		    steps{
			  echo'Test'
		         }
		}
		stage('Deploy'){
		      steps{
			  echo'Deploy'
			}
		}
          }
          post{
               always{
                      echo'always called'
               }
               success{
                      echo'Sucess Called'                     
               }
               failure{
                      echo'failure called'
               }
            }
}
